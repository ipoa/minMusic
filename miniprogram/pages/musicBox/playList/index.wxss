/* pages/template/playList/index.wxss */
.swiperimg .image {
  width: 100%;
  height: 100%;
}
.swiperimg{
  height: 300rpx;
}
.playList-wrapper{
  margin-top: 10px;
  height: calc(100vh - 150rpx - 48px );
  height: calc(100vh - 150rpx - 48px - constant(safe-area-inset-bottom));
  height: calc(100vh - 150rpx - 48px - env(safe-area-inset-bottom));
}
.playList-container{
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;
  margin-top: 10rpx;
  flex-direction: row;
  padding-bottom: 88px;
}